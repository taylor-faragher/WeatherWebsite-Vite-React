{
    "compilerOptions": {
        "target": "es5",
        "lib": ["esnext", "dom"],
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictPropertyInitialization": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "types": ["cypress"]
    }
}
